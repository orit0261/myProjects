                                                p_accepted boolean)
    returns TABLE
            (
                p_document_serial integer
            )
    language sql
as
$$
UPDATE prod.document_exceptions
SET accepted=p_accepted
WHERE ("tenantId" = p_tenantid)
  AND ("document_serial" = p_document_serial)
  AND ("exceptionId" = p_exception_id)

RETURNING "document_serial";

$$;

